{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.3.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "aego/oauth2-yandex": "^0.2.2",
    "ilukyanov/oauth2-odnoklassniki": "^0.4.2",
    "knplabs/knp-paginator-bundle": "^4.0",
    "knpuniversity/oauth2-client-bundle": "^1.32",
    "league/oauth2-facebook": "^2.0",
    "league/oauth2-instagram": "^2.0",
    "liip/imagine-bundle": "^2.2",
    "nette/utils": "^3.0",
    "oneup/flysystem-bundle": "^3.1",
    "phpoffice/phpspreadsheet": "^1.10",
    "ramsey/uuid": "^3.8",
    "sensio/framework-extra-bundle": "^5.4",
    "stof/doctrine-extensions-bundle": "^1.3",
    "symfony/asset": "4.3.*",
    "symfony/console": "4.3.*",
    "symfony/dotenv": "4.3.*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "4.3.*",
    "symfony/framework-bundle": "4.3.*",
    "symfony/mailer": "4.3.*",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "4.3.*",
    "symfony/serializer-pack": "^1.0",
    "symfony/translation": "4.3.*",
    "symfony/twig-bundle": "4.3.*",
    "symfony/validator": "4.3.*",
    "symfony/webpack-encore-bundle": "^1.6",
    "symfony/yaml": "4.3.*",
    "twig/extensions": "^1.5",
    "zablik/oauth2-vkontakte": "^1.2"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.2",
    "fzaninotto/faker": "^1.8",
    "symfony/debug-pack": "^1.0",
    "symfony/maker-bundle": "^1.12",
    "symfony/phpunit-bridge": "^4.3",
    "phpunit/phpunit": "^8.4",
    "symfony/profiler-pack": "^1.0",
    "symfony/test-pack": "^1.0",
    "symfony/var-dumper": "4.3.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.3.*"
    }
  }
}
