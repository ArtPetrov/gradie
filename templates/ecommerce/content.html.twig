    'ecommerce/_form_radio.html.twig',
    'ecommerce/group/_form_products_entry.html.twig',
    'ecommerce/group/_form_selectors_entry.html.twig',
    'ecommerce/category/_form_filters_entry.html.twig',
    'ecommerce/product/_form_recommended_entry.html.twig',
    'ecommerce/product/_form_composition_entry.html.twig',
    'ecommerce/product/_form_images_entry.html.twig',
    'ecommerce/product/_form_categories_categories_choice.html.twig',
